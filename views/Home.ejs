<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="/send" method="post">
        <input type="text" placeholder="UserName" name="username">
        <input type="text" placeholder="Principleamount" name="principleamount">
        <input type="text" placeholder="Month" name="month">
        <input type="text" placeholder="Intrest" name="intrest">

        <button type="submit">Submit</button>

    </form>

    <table border="1px">
        <tr>
            <th>UserName</th>
            <th>PrincipleAmount</th>
            <th>Month</th>
            <th>Intrest</th>
            <th>Action</th>
        </tr>
        <%data.map((e,i)=>{ %>
            <tr key={i}>
                <td><%=e.username%></td>
                <td><%=e.principleamount%></td>
                <td><%=e.month%></td>
                <td><%=e.intrest%></td>
                <td>
                    <a href="/edit?id=<%=e.id%>"><button>Edit</button></a>
                   <a href="/delete?id=<%=e.id%>"> <button>Delete</button></a>
                </td>
            </tr>
       <% })%>
    </table>
</body>
</html>